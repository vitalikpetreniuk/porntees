	}
	body.homepage
	{
		.homepage-header
		{
			padding: 12px 0;

			a.hh-tablet
			{
				span:last-child
				{
					margin: 0 4px;
				}
			}
		}
		.homepage-header>a>span
		{
			font-size: 24px;
			line-height: 1;
		}
		.tees-slider .ts-item
		{
			height: calc(100vh - 48px);
		}
		.tees-slider-counter
		{
		    font-size: 24px;
			line-height: 1;

			right: 2.15vw;
			bottom: 1.95vw;

			span
			{
				height: 24px;
				width: 3vw;
			 	font-size: 24px;
				line-height: 24px;
			}
			span.current
			{
				margin-right: 0;

				span
				{
					position: absolute;
					right: 0;

					display: block;
					line-height: 1;
				}
				span:nth-child(1)
				{
					top: 0;
				}
				span:nth-child(2)
				{
					top: 24px;
				}
			}
			span.current.changing
			{
				span
				{
					-webkit-transition: all .15s linear;
					transition: all .15s linear;

					position: absolute;
					right: 0;

					display: block;
					line-height: 24px;
				}
				span:nth-last-child(2)
				{
					top: -24px;
				}
				span:nth-last-child(1)
				{
					top: 0;
				}
			}
		}
	}


