			    span
			    {
			    	display: block;
			    	position: absolute;
			    	left: 0;
			    	right: 0;

			    	text-align: center;
			    	line-height: 1;
			    }
			    span:nth-child(1)
			    {
			    	top: 10px;
			    }
			    span:nth-child(2)
			    {
			    	top: 40px;
			    }
			}

			button
			{
				width: 180px;
				height: 40px;

				font-size: 20px;
				line-height: 1;

				span
				{

					top: 40px;
				}
			}
			button.add-to-cart
			{
				span.in-cart
				{
					top: 40px;
					-webkit-transition: none;
					transition: none;
				}
			}
			button.adding
			{
				span.add-to-cart
				{
					top: 40px;
					-webkit-transition: none;
					transition: none;
				}
			}
			button.in-cart
			{
				span.add-to-cart,
				span.adding
				{
					top: 40px;
					-webkit-transition: none;
					transition: none;
				}
			}
			button.adding.loading
			{
				span.add-to-cart
				{
					top: -20px;
					-webkit-transition: all .15s linear;
					transition: all .15s linear;
				}
			}
			button.in-cart.loading
			{
				span.adding
				{
					top: -20px;
					-webkit-transition: all .15s linear;
					transition: all .15s linear;
				}
			}
			button.add-to-cart.loading
			{
				span.in-cart
				{
					top: -20px;
					-webkit-transition: all .15s linear;
					transition: all .15s linear;
				}
			}
