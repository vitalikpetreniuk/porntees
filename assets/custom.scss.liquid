@font-face {
  font-family: 'FoundersGroteskRegular';
  src: url('FoundersGroteskRegular.ttf') format('truetype'),
       url('FoundersGroteskRegular.woff') format('woff'),
       url('FoundersGroteskRegular.woff2') format('woff2');
  font-weight: normal;
  font-style: normal;
}
h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6
{
  	font-family: 'FoundersGroteskRegular';
  	font-weight: normal;
  	font-style: normal;
  	letter-spacing: 0;

  	font-size: 1.87vw;
    line-height: 1.7vw;
}
a
{
    line-height: 1.7vw;

	text-decoration: none;
	border: none;
	outline: none; 
	-webkit-transition: all .15s linear;	
	transition: all .15s linear;
}
a:hover, li.active a
{
	color: #EB3B2D;
}
button
{
  	font-family: 'FoundersGroteskRegular';
	color: #000000;
	letter-spacing: 0;

	font-size: 24px;
	line-height: 1;
}

h2
{
  	font-size: 1.87vw;
    line-height: 1.7vw;
}

body
{
	background: #FFFAF4;
  	font-family: 'FoundersGroteskRegular';
  	font-weight: normal;
  	font-style: normal;

  	font-size: 1.87vw;
    line-height: 1.7vw;
  	letter-spacing: 0;
  	color: #000000;

	.site-header
	{
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		z-index: 101;

		background: #FFFAF4;
		padding: 1.48vw 1.45vw 1.13vw;
		border-bottom: 1px solid #000000;

		.grid
		{
			background: transparent;
			margin-left: 0;
			display: flex;
			flex-wrap: wrap;
			justify-content: space-between;
			align-items: center;

			.grid-item
			{
				padding: 0;
				display: block;

				.tees-logo
				{
					margin-right: 1.04vw;
					display: block;

					.h2.site-header__logo
					{
						line-height: 1;

						a
						{
							display: block;
						}
						a:hover
						{
							color: #EB3B2D;
						}
					}
				}
				.tees-nav
				{
					display: none;
					-webkit-transition: all .15s linear;
					transition: all .15s linear;

					ul
					{
						display: flex;
						flex-wrap: wrap;
						justify-content: flex-start;

					 	li
						{
							display: block;
							margin-right: 1.04vw;

							a
							{
								display: block;
								padding: 0;
								color: #000000;

								span
								{
									display: block;
									line-height: inherit;
								}
							
							}
						}
						li.active a
						{
							color: #EB3B2D;
						}
						li:last-child
						{
							margin-right: 0;
						}
					} 

					.tees-nav_link.active,
					.tees-nav_link:hover
					{
						color: #EB3B2D;
					}
				}
				.tees-nav.loading
				{
					opacity: 0;

					-webkit-transition: all .15s linear;
					transition: all .15s linear;
				}
				.tees-nav.tees-subnav-show
				{
					ul li:not(.active)
					{
						display: none;
					}
					ul li.active a
					{
						margin-right: 1.04vw;
					}
					ul li.active .tees-nav_tags
					{
						display: flex !important;

						span
						{
							margin-right: 1.04vw;
							text-transform: uppercase;
							cursor: pointer;
						}
						span:hover,
						span.active
						{
							color: #EB3B2D;
						}
						span.non-active-hovered:hover
						{
							color: #000000 !important;
						}

						/* span[data-tag="#s"]
						{
							order: 0;
						}
						span[data-tag="#m"]
						{
							order: 1;
						}
						span[data-tag="#l"]
						{
							order: 2;
						}
						span[data-tag="#xl"]
						{
							order: 3;
						}
						span[data-tag="#xxl"]
						{
							order: 4;
						} */
					}
					ul li.active
					{
						display: flex;
						justify-content: flex-start;
					}
				}

				.h2.site-header__logo,
				ul.site-nav
				{
					margin: 0;
				}
				ul.site-nav a .site-nav__label
				{
					border: none !important;
				}

				.site-header__cart
				{
					display: block;
					padding: 0;

					.cart-title
					{
						display: none;
						margin-left: 1.04vw;
						color: #000000;
					}
					.cart-title:hover
					{
						color: #EB3B2D;
					}

					#CartCount
					{
						display: block;
						background: none;
						border-radius: none;
						width: auto;
						height: auto;

						position: static;

						div
						{
							text-align: left;
							position: relative;
							overflow: hidden;
							height: 1.7vw;
							min-width: 3.3vw;

							span
							{
								position: absolute;
								right: 0;

								display: block;
    							line-height: 1.7vw;
								color: #EB3B2D;
							}
							span:nth-child(1)
							{
								top: 0;
							}
							span:nth-child(2)
							{
								top: 27px;
							}
						}
						div.changing
						{
							span
							{
								-webkit-transition: all .15s linear;
								transition: all .15s linear;
							}
							span:nth-last-child(2)
							{
								top: -27px;
							}
							span:nth-last-child(1)
							{
								top: 0;
							}
						}

					    span
					    {
							font-family: 'FoundersGroteskRegular', sans-serif;
						    font-weight: normal;
						    font-style: normal;
						    font-size: 1.87vw;
							line-height: 1.7vw;
						    text-transform: uppercase;
							color: #3d4246;
					    }
					}
				}
				.site-header__cart.empty
				{
					.cart-title
					{
						display: block;
					}
					#CartCount
					{
						display: none;
					}
				}
			}
			.grid-item-left
			{
				display: flex;
				justify-content: flex-start;
			}
			.grid-item-right
			{
				nav
				{
					display: none;

					ul li a
					{
						color: #000000;
					}
					ul li a:hover,
					ul li.active a
					{
						color: #EB3B2D;
					}
				}
				.open-info
				{
					display: block;
					margin-left: 1.04vw;

					cursor: pointer;
				}
				.open-info:hover
				{
					color: #EB3B2D;
				}
			}
		}
		.grid::after
		{
			display: none;
		}
	}
	.site-header.info-opened
	{
		.grid
		{
			.grid-item-left
			{
				.tees-nav
				{
					display: none !important;
				}
			}
			.grid-item-right
			{
				nav
				{
					display: block;
				}
				.open-info
				{
					display: none;
				}
			}
		}
	}
	.homepage-header
	{
		display: none;
	}

	.main-content
	{
		min-height: calc(100vh - 4.27vw);
	}

	#shopify-section-footer
	{
		display: block;

		.site-footer
		{
			background: #FFFAF4;
			padding: 2.6vw 2.5vw 2.08vw;
			margin: 0;
			border-top: 1px solid #000000;
			color: #000000;

			a
			{
				color: #000000;
			}
			a:hover
			{
				color: #EB3B2D;
			}

			.page-width:first-child
			{
				max-width: none;
				padding: 0;

				.site-footer__content
				{
					.site-footer__item
					{
						margin: 0;
						flex: auto;

						p.h4
						{
							display: none;
						}
						p
						{
							font-size: 1.25vw;
							line-height: 1.41vw;
							text-transform: uppercase;
							padding: 0;
							margin: 0;
							color: #000000;

							a
							{
								border: none !important;
							}
							strong
							{
								font-weight: normal;
								cursor: pointer;
							}
							strong:hover, strong:focus
							{
								color: #EB3B2D;
							}
						}
					}
					.site-footer__item:nth-child(1)
					{
						width: 14.5vw;

						ul li
						{
							display: block;
							padding: 1px 0;
							height: 1.41vw;
							line-height: 1;

							a
							{
								display: block;
								font-size: 1.25vw;
								line-height: inherit !important;
								text-transform: uppercase;

								color: #000000;
							}
							a:hover
							{
								color: #EB3B2D;
								border-bottom: none !important;
							}
						}
					}
					.site-footer__item:nth-child(2)
					{	
						width: 56vw;
						padding-right: 15vw;
						box-sizing: border-box;
					}
					.site-footer__item:nth-child(3)
					{
						width: 23vw;

						.contact-form
						{
							display: none;
						}
						a
						{
							line-height: 1.41vw !important;
						}
					}
					.site-footer__item:nth-child(3).active
					{
						p:nth-child(3)
						{
							display: none;
						}
						.contact-form
						{
							display: block;
						}
					}
					.site-footer__item:nth-child(3) > div
					{
						width: 100%;
					}
					.site-footer__item:nth-child(4)
					{
						display: none;
					}

					.contact-form
					{
						.input-group 
						{
							justify-content: flex-start;
						}
						input[type="email"],
						button.btn.newsletter__submit
						{
							margin-top: 0;
							flex-grow: unset;
							min-height: 1px;
						}
						input[type="email"]
						{
							margin: 0;
							display: block;
							border: none;
							outline: none;
							background: transparent;
							color: #000000;
							font-size: 1.25vw;
							line-height: 1.41vw;
							height: 1.41vw;
							box-sizing: border-box;
							text-transform: uppercase;
							opacity: 1;
							padding: 0;

							flex-basis: unset;
							width: calc(100% - 7.6vw);
							border-bottom: 1px solid #000000;
						}
						input[type="email"]::placeholder
						{
							color: #000000 !important;
							opacity: 1;
							font-size: 1.25vw;
							text-transform: uppercase;
						}
						.input-group__btn
						{
							height: 1.41vw;
							line-height: 1.41vw;
							flex-grow: unset;
							margin-left: 5px;
						}
						button.btn.newsletter__submit
						{
							display: block;
							padding: 0;
							background: transparent;
							border: none;
							outline: none;
							color: #000000;
							letter-spacing: 0;
							height: 1.41vw;

							font-size: 1.25vw;
							line-height: 1;
						}
					}
				}
			}
			.site-footer__hr,
			.site-footer__hr + div.page-width
			{
				display: none;
			}
		}
	}
}
