body.ajax-products-list
{
	height: 100%;
	overflow: hidden;
}

.main-content
{
	padding: 0;
}
#shopify-section-collection-template
{
	.page-width
	{
		max-width: none;
		padding: 0 1.05vw;
	}
}

#PageContainer
{
	-webkit-transform: none !important;
	transform: none !important;
}

.products-list
{
	min-height: 100vh;
	padding-top: 4.27vw;
	box-sizing: border-box;

	.product-item
	{
		-webkit-transition: opacity .15s linear;
		transition: opacity .15s linear;
	}
}

.products-list.loading
{
	position: relative;

	.product-item 
	{
		opacity: 0;
		visibility: hidden;

		-webkit-transition: opacity .05s linear;
		transition: opacity .05s linear;
	}
}
.products-list.loading:before
{
	content: '';
	display: block;
	position: absolute;
	width: 60px;
	height: 60px;

	top: calc(50vh + 75px - 30px);
	left: 50%;

	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);

	background: url('preloader.svg') no-repeat center center;
	background-size: 100%;
}
.products-list-loaded
{
	opacity: 0;
	visibility: hidden;

	background: #FFFAF4;
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	padding: 75px 20px 0;
	box-sizing: border-box;
	height: 100vh;
	width: 100%;
	overflow-x: hidden;
	overflow-y: scroll;
}
.products-list-loaded.loaded-items
{
	opacity: 1;
	visibility: visible;
	z-index: 100;
}
.product-item
{
	margin-bottom: 90px;
	position: relative;

	.product-gallery
	{
		position: relative;
		width: calc(100% + 20px);

		.owl-item
		{
			outline: none !important;
			border: none !important;
		}
		.item
		{
			height: 42.7vw;
			width: 87.5vw;
			outline: none !important;

			img
			{
				display: block;
				height: 100%;
				width: 65vw;
				object-fit: cover;
				outline: none;
			}
		}

		.owl-nav
		{
			button
			{
				position: absolute;
				top: 0;
				bottom: 0;
				width: 20vw;
				z-index: 10;

				outline: none !important;
				border: none !important;
			}
			button.owl-prev
			{
				left: 0;
			}
			button.owl-next
			{
				right: 0;
			}
			button.disabled
			{
				display: none !important;
			}
		}
	}

	.product-info
	{
		margin-top: 1.45vw;
		max-width: 65vw;
		display: flex;
		flex-wrap: wrap;
		justify-content: space-between;
		align-items: center;
		text-transform: uppercase;

		.pi-left
		{
			display: flex;
			flex-wrap: wrap;
			justify-content: flex-start;
			align-items: center;

			.product-title
			{
				margin: 0 1.52vw 0 0;
				font-weight: normal;
				letter-spacing: 0 !important;
			  	font-size: 1.87vw;
    			line-height: 1.7vw;

				color: #000000;
			}
			.product-price
			{
			  	font-size: 1.87vw;
    			line-height: 1.7vw;
				color: #000000;

				span
				{
					padding-right: 5px;
				}
			}
		}
		.pi-right
		{
			button
			{
				padding: 0;
				outline: none;
				border: none;
			  	font-size: 1.87vw;
    			line-height: 1.7vw;

				height: 1.87vw;
				color: #000000;
				background: none;
				text-transform: uppercase;
			}
			button:hover
			{
				color: #EB3B2D;
			}
		}
	}

	.product-gallery-counter
	{
		position: absolute;
		bottom: 0;
		right: -1.57vw;
		width: 12.5vw;

		font-size: 1.87vw;
		line-height: 1.7vw;

		color: #000000;
		overflow: hidden;
		display: flex;

		span
		{
			height: 1.7vw;
			width: 1.57vw;
			font-size: 1.87vw;
			line-height: 1.7vw;
			display: block;
		}
		span.current
		{
			text-align: right;
			position: relative;
			margin-right: 3px;

			span
			{
				position: absolute;
				right: 0;

				display: block;
				line-height: 1;
			}
			span:nth-child(1)
			{
				top: 0;
			}
			span:nth-child(2)
			{
				top: 1.7vw;
			}
		}
		span.current.changing
		{
			span
			{
				-webkit-transition: all .15s linear;
				transition: all .15s linear;

				position: absolute;
				right: 0;

				display: block;
				line-height: 1.7vw;
			}
			span:nth-last-child(2)
			{
				top: -1.7vw;
			}
			span:nth-last-child(1)
			{
				top: 0;
			}
		}
